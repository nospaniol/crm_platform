                                <div class="form-group">
                                    <input  class="form-control" type="number" placeholder=Cost Per Car" name="costPerCar" id="costPerCar">
                                </div>
                                <div class="form-group">
                                    <input  class="form-control" type="number" placeholder="Monthly Cost" name="monthlyCost" id="monthlyCost">
                                </div> 
                                <div class="form-group">
                                    <input  class="form-control" type="number" placeholder="Current Spend" name="currentSpend" id="currentSpend">
                                </div>
                                <div class="kt-login__actions">
                                    <button  id="kt_roi_calculate_submit" class="btn btn-brand btn-pill btn-elevate">Calculate</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
